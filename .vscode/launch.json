{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Run App",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}/conversation_editor",
      "program": "app.py",
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}
